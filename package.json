{
  "name": "rezumator",
  "version": "1.0.0",
  "description": "cv maker",
  "scripts": {
    "start": "run-p client server",
    "client": "cd client && pnpm dev",
    "server": "cd server && pnpm start:dev",
    "deps": "run-p client-deps server-deps",
    "client-deps": "cd client && pnpm i",
    "server-deps": "cd server && pnpm i",
    "client-prod": "cd client && vercel --prod",
    "server-prod": "cd server && vercel --prod"
  },
